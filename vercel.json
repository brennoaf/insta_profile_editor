{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/scraped-profile", "dest": "/api/scraped-profile.js" },
    { "src": "/api/scraped-publications", "dest": "/api/scraped-publications.js" },
    { "src": "/api/proxy-instagram(.*)", "dest": "/api/proxy-instagram.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
